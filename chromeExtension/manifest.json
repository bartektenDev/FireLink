{
  "name": "FireLink",
  "version": "1.0",
  "description": "Share links from your from Google Chrome to your Android device in seconds!",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "FireLink",
    "default_icon": {
      "16": "images/web_hi_res_512.png",
      "32": "images/web_hi_res_512.png",
      "48": "images/web_hi_res_512.png",
      "128": "images/web_hi_res_512.png"
    }
  },
  "icons": {
      "16": "images/web_hi_res_512.png",
      "32": "images/web_hi_res_512.png",
      "48": "images/web_hi_res_512.png",
      "128": "images/web_hi_res_512.png"
  },
  "permissions": [
      "tabs", "identity", "storage"
  ],
  "key": "",
  "content_security_policy":"script-src 'self' https://cdn.firebase.com https://*.firebaseio.com/ https://apis.google.com https://www.gstatic.com/ ; object-src 'self'",
  "manifest_version": 2
}
